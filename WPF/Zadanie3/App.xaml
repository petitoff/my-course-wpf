<Application x:Class="Zadanie3.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Zadanie3"
             xmlns:converter="clr-namespace:Zadanie3.Converter"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <converter:StateOfFieldConverter x:Key="StateOfFieldConverter" />
        <converter:IShootButtonConverter x:Key="IShootButtonConverter" />
        <converter:PlayerTurnConverter x:Key="PlayerTurnConverter" />
        <converter:PlayerWinConverter x:Key="PlayerWinConverter" />
        <converter:IsPlayerReadyTextConverter x:Key="IsPlayerReadyTextConverter" />
    </Application.Resources>
</Application>